{
  "manifest_version": 3,
  "name": "Termzinator",
  "version": "1.0.0",
  "description": "Making the internet a more fair and equitable place for everyone.",
  "action": {
    "default_popup": "./popup.html"
  },
  "permissions": ["scripting", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ]
}
